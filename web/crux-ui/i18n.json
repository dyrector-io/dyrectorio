{
  "logBuild": false,
  "locales": ["en"],
  "defaultLocale": "en",
  "pages": {
    "*": ["common", "head", "errors", "badges"],
    "/404": ["404"],
    "/500": ["500"],
    "/auth/login": ["login"],
    "/auth/recovery": ["recovery"],
    "/auth/register": ["register"],
    "/auth/create-account": ["create-account"],
    "/auth/settings": ["settings"],
    "/auth/settings/edit-profile": ["settings"],
    "/auth/settings/change-password": ["settings"],
    "/auth/settings/new-password": ["settings"],
    "/auth/verify": ["verify"],
    "/products": ["products"],

    "/products/[productId]": ["products", "versions", "images", "deployments"],
    "/products/[productId]/versions/[versionId]": ["versions", "images", "deployments"],

    "/products/[productId]/versions/[versionId]/deployments/[deploymentId]": ["images", "deployments", "nodes"],
    "/products/[productId]/versions/[versionId]/deployments/[deploymentId]/deploy": ["deployments"],
    "/products/[productId]/versions/[versionId]/deployments/[deploymentId]/instance/[instanceId]": [
      "images",
      "deployments",
      "container"
    ],
    "/products/[productId]/versions/[versionId]/image/[imageId]": ["images", "container"],
    "/nodes": ["nodes"],
    "/nodes/[nodeId]": ["nodes", "images"],
    "/registries": ["registries"],
    "/registries/[registryId]": ["registries"],
    "/teams": ["teams"],
    "/teams/[teamId]": ["teams"],
    "/teams/create": ["teams"],
    "/teams/[teamId]/invitation": ["teams"],
    "/audit-log": ["audit"],
    "/notifications": ["notifications"],
    "/notifications/[notificationId]": ["notifications"],
    "/deployments": ["deployments"],
    "/status": ["status"],
    "/templates": ["templates", "products"],
    "/dashboard": ["dashboard"]
  }
}
