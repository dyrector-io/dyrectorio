{
  "name": "Google Microservices Demo",
  "description": "Google uses this application to demonstrate use of technologies like Kubernetes/GKE, Istio, Stackdriver, gRPC and OpenCensus.",
  "registries": [
    {
      "name": "Docker Hub Library",
      "type": "hub",
      "hub": {
        "imageNamePrefix": "library"
      }
    },
    {
      "name": "Google Microservices Demo",
      "description": "Google uses this application to demonstrate use of technologies like Kubernetes/GKE, Istio, Stackdriver, gRPC and OpenCensus.",
      "type": "google",
      "google": {
        "url": "gcr.io",
        "imageNamePrefix": "google-samples/microservices-demo"
      }
    }
  ],
  "images": [
    {
      "name": "adservice",
      "registryName": "Google Microservices Demo",
      "image": "adservice",
      "tag": "v0.4.1",
      "config": {
        "name": "adservice",
        "ports": [
          {
            "external": 9555,
            "internal": 9555
          }
        ],
        "environment": [
          {
            "key": "PORT",
            "value": "9555"
          },
          {
            "key": "DISABLE_STATS",
            "value": "1"
          },
          {
            "key": "DISABLE_TRACING",
            "value": "1"
          }
        ],
        "networks": ["google-microservices-demo-network"]
      }
    },
    {
      "name": "cartservice",
      "registryName": "Google Microservices Demo",
      "image": "cartservice",
      "tag": "v0.4.1",
      "config": {
        "name": "cartservice",
        "ports": [
          {
            "external": 7070,
            "internal": 7070
          }
        ],
        "environment": [
          {
            "key": "REDIS_ADDR",
            "value": "redis:6379"
          }
        ],
        "networks": ["google-microservices-demo-network"]
      }
    }
  ]
}
