{
  "name": "crux",
  "version": "0.10.0",
  "description": "Open-source delivery platform that helps developers to deliver applications efficiently by simplifying software releases and operations in any environment.",
  "author": "dyrector.io",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyrector-io/dyrectorio.git"
  },
  "bugs": {
    "url": "https://github.com/dyrector-io/dyrectorio/issues"
  },
  "homepage": "https://github.com/dyrector-io/dyrectorio#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "encrypt:generate": "node dist/main encrypt generate",
    "encrypt:rotate": "node dist/main encrypt rotate",
    "grpc:debug": "GRPC_VERBOSITY=DEBUG GRPC_TRACE=all npm start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" && prettier --check \"src/**/*.ts\"",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --detectOpenHandles --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --config ./jest.config.js --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "ts-node": "ts-node --compiler-options { \"module\": \"CommonJS\"}",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate deploy"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.1.3",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-ws": "^10.1.3",
    "@nestjs/swagger": "^7.1.6",
    "@nestjs/websockets": "^10.1.3",
    "@ory/kratos-client": "^0.13.1",
    "@prisma/client": "5.1.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/semver": "^7.5.0",
    "@types/ws": "^8.5.5",
    "@willsoto/nestjs-prometheus": "^5.2.1",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "google-auth-library": "^9.0.0",
    "google-protobuf": "^3.21.2",
    "handlebars": "^4.7.8",
    "nestjs-pino": "^3.3.0",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.2",
    "pino-http": "^8.3.3",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "semver": "^7.5.4",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "www-authenticate": "^0.6.3",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/nodemailer": "^6.4.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.6.2",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prisma": "5.1.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "overrides": {
    "degenerator": "5.0.0"
  }
}
