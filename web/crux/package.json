{
  "name": "crux",
  "version": "0.4.1",
  "description": "Open-source delivery platform that helps developers to deliver applications efficiently by simplifying software releases and operations in any environment.",
  "author": "dyrector.io",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyrector-io/dyrectorio.git"
  },
  "bugs": {
    "url": "https://github.com/dyrector-io/dyrectorio/issues"
  },
  "homepage": "https://github.com/dyrector-io/dyrectorio#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "grpc:debug": "GRPC_VERBOSITY=DEBUG GRPC_TRACE=all npm start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --config ./jest.config.js --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "test:crux": "npm run docker:up && npx prisma migrate deploy && jest -i && npm docker:down",
    "ts-node": "ts-node --compiler-options { \"module\": \"CommonJS\"}",
    "database:up": "docker-compose -f docker-compose.database.yaml up -d",
    "database:down": "docker-compose -f docker-compose.database.yaml down",
    "prismagen": "prisma generate"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.6",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/platform-ws": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/websockets": "^9.4.0",
    "@ory/kratos-client": "^0.11.1",
    "@prisma/client": "4.13.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/ws": "^8.5.4",
    "@willsoto/nestjs-prometheus": "^5.1.1",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "google-auth-library": "^8.7.0",
    "google-protobuf": "^3.21.2",
    "handlebars": "^4.7.7",
    "nestjs-pino": "^3.2.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.10.0",
    "pino-http": "^8.3.3",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "www-authenticate": "^0.6.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.2",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "prisma": "4.13.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
