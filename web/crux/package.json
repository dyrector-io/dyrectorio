{
  "name": "crux",
  "version": "0.1.1",
  "description": "Open-source delivery platform that helps developers to deliver applications efficiently by simplifying software releases and operations in any environment.",
  "author": "dyrector.io",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyrector-io/dyrectorio.git"
  },
  "bugs": {
    "url": "https://github.com/dyrector-io/dyrectorio/issues"
  },
  "homepage": "https://github.com/dyrector-io/dyrectorio#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:proto": "find proto -name '*.proto' -exec protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=nestJs=true --ts_proto_opt=addNestjsRestParameter=true --ts_proto_opt=outputJsonMethods=true --ts_proto_opt=addGrpcMetadata=true --ts_proto_out=./src/proto/ {} \\;",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/seeds/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "grpc:debug": "GRPC_VERBOSITY=DEBUG GRPC_TRACE=all npm start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --config ./jest.config.js --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "ts-node": "ts-node --compiler-options { \"module\": \"CommonJS\"}",
    "database:up": "docker-compose -f docker-compose.database.yaml up -d",
    "database:down": "docker-compose -f docker-compose.database.yaml down",
    "test:crux": "npm run docker:up && npx prisma migrate deploy && jest -i && npm docker:down",
    "prismagen": "prisma generate",
    "compodoc": "compodoc -p tsconfig.doc.json --theme stripe",
    "demo-seed": "npx ts-node prisma/seeds/demo-seeder.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seeds/seeder.ts",
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.8",
    "@grpc/proto-loader": "^0.7.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.0.8",
    "@nestjs/platform-express": "^9.0.8",
    "@ory/kratos-client": "^0.10.1",
    "@prisma/client": "4.1.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/google-protobuf": "^3.15.6",
    "@willsoto/nestjs-prometheus": "^4.7.0",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "google-auth-library": "^8.1.1",
    "google-protobuf": "^3.21.0",
    "handlebars": "^4.7.7",
    "pg": "^8.7.3",
    "prom-client": "^14.0.1",
    "protobufjs": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "www-authenticate": "^0.6.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prisma": "4.1.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-proto": "1.125.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  }
}
